    {
        "uri": "configuration/entityTypes/Organization",
        "permissions": [
            {
                "role": "ROLE_SE_ADMIN",
                "access": [
                    "CREATE",
                    "READ",
                    "UPDATE",
                    "MERGE",
                    "UNMERGE",
                    "DELETE"
                ]
            },
            {
                "role": "ROLE_SE_BUSINESS_USER",
                "access": [
                    "READ"
                ]
            },
            {
                "role": "ROLE_SE_DATASTEWARD",
                "filter": "equals(attributes.KeyAccountType,'SA') or equals(attributes.KeyAccountType,'OSA') or equals(attributes.KeyAccountType,'MCTA') or equals(attributes.KeyAccountType,'ID') ",
                "access": [
                    "READ",
                    "CREATE",
                    "UPDATE",
                    "MERGE"
                ]
            },
            {
                "role": "ROLE_SE_DS",
                "filter": "equals(attributes.KeyAccountType,'TA') or equals(attributes.KeyAccountType,'NA') or equals(attributes.KeyAccountType,'CTAC') ",
                "access": [
                    "READ",
                    "CREATE",
                    "UPDATE",
                    "MERGE",
                    "UNMERGE",
                    "DELETE"
                ]
            }
        ]
    },
